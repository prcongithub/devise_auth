<!DOCTYPE html>
<html>
<head>
  <title>Online Orders Demo | <%= title %></title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag("application") %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<%= render('layouts/header') %>
		<section>
			<%= render('layouts/sidebar') %>
			<div class="content round">
				<div>
					<%= notice %>
					<%= alert %>
				</div>
				<%= yield %>
			</div>
			<div class="clr"></div>
		</section>	
		<%= render('layouts/footer') %>
	</div>
</body>
</html>
